//helpers
function compare(a,b)
	if (a>b) return 1 
	if (a<b) return -1 
	return 0
end

function fuzzycompare(a,b,fuzz)
	if(rnd(100)<fuzz) return flr(rnd(3))-1
	return compare(a,b)
end

function pickframe(frames, cps)
 ms= time()*1000%1000
	frameduration=1000/cps
	currentframe=flr(ms/frameduration%#frames)
	return frames[currentframe+1]
end

function collide(obj, other)
    if
        other.pos.x+other.hitbox.x+other.hitbox.w > obj.pos.x+obj.hitbox.x and 
        other.pos.y+other.hitbox.y+other.hitbox.h > obj.pos.y+obj.hitbox.y and
        other.pos.x+other.hitbox.x < obj.pos.x+obj.hitbox.x+obj.hitbox.w and
        other.pos.y+other.hitbox.y < obj.pos.y+obj.hitbox.y+obj.hitbox.h 
    then
        return true
    end
end

function getunitvector(obj, other)
  local dx = other.pos.x - obj.pos.x
  local dy = other.pos.y - obj.pos.y
  
  -- calculate the magnitude of the vector
  local magnitude = sqrt(dx^2 + dy^2)
  
  -- calculate the unit vector components
  local unitvector = {}
  unitvector.x = dx / magnitude
  unitvector.y = dy / magnitude
  
  return unitvector
end

function randomborderpoint(rectanglewidth, rectangleheight)
  -- generate a random side (1 to 4) to determine which border
  local randomside = flr(rnd(4)) + 1  
  -- generate a random position along the chosen border
  local randomx, randomy
  if randomside == 1 then  -- top border
    randomx = rnd(1) * rectanglewidth
    randomy = 0
  elseif randomside == 2 then  -- right border
    randomx = rectanglewidth
    randomy = rnd(1) * rectangleheight
  elseif randomside == 3 then  -- bottom border
    randomx = rnd(1) * rectanglewidth
    randomy = rectangleheight
  else  -- left border
    randomx = 0
    randomy = rnd(1) * rectangleheight
  end

  return {x = randomx, y = randomy}
end

function copy(orig)
    local orig_type = type(orig)
    local copy
    if orig_type == 'table' then
        copy = {}
        for orig_key, orig_value in pairs(orig) do
            copy[orig_key] = orig_value
        end
    else -- number, string, boolean, etc
        copy = orig
    end
    return copy
end

function onscreen(obj)
	  if obj.pos.x >= 0 and obj.pos.x <= 127 and obj.pos.y >= 0 and obj.pos.y <= 127 then
    return true
  else
    return false
  end
end

function findclosest(pos, item_list)
  local closest_item = nil
  local closest_distance = 9999 -- set an initial high value
  
  -- iterate through the list of items
  for i = 1, #item_list do
    local item = item_list[i]
    local item_x = item.pos.x
    local item_y = item.pos.y
    
    -- calculate the distance between the target and the current item
    local distance = sqrt((pos.x - item_x)^2 + (pos.y - item_y)^2)
    
    -- update the closest item if the current item is closer
    if distance < closest_distance then
      closest_item = item
      closest_distance = distance
    end
  end
  
  return closest_item
end
