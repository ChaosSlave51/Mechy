//events
function _init()
	game = newgame()
	music(1,0,2)
end

function _update()
	game.player:update()
 for bullet in all(game.bullets) do
 	bullet:update()
	end
	game.hammer:update()
	
	for enemy in all(game.enemies) do
		enemy:update()		
	end
	game:update()	
end

function _draw()
//map
	map( 0, 0, 0, 0, 128, 64 )
	map( 0, 0, 0, 64, 128, 64 )
	
	game.player:draw()
	
	//bullets
	for bullet in all(game.bullets) do
		bullet:draw()
	end	
	game.hammer:draw()	
	//enemies
	for enemy in all(game.enemies) do
		enemy:draw()
	end
	//ui
	rectfill(0,0,game.player.life*2,4,10)
	print(game.score,70,0,9)
end




