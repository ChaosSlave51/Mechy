//bullet
function newbullet(pos,vector)
		return {
		pos=copy(pos),
		hitbox={x=3,y=3,h=2,w=2},
		speed=2,
		sprites={48,49},
		cps=3,
		vector=copy(vector),
		update=function(self)
			self.pos.x+=self.speed*self.vector.x
			self.pos.y+=self.speed*self.vector.y
			if(onscreen(self)==false)then
				del(bullets,self)
			end
			damageenemies(self)
			
		end,		
			
		draw=function(self)
			spr(pickframe
				(self.sprites,self.cps)
					,self.pos.x,self.pos.y)
		end,
		damage=function(self)
			del(game.bullets,self)
		end
			
	}
end

function newhammer()
	return {
		pos={x=0,y=0},
		hitbox={x=3,y=3,h=4,w=4},
		speed=0.02,
		cps=1,
		sprites={16},
		radius=10,
		angle=0,
		iframes=30,
		invincible=0,
		update=function(self)
			self.invincible-=1
			self.angle = self.angle + self.speed
	 	self.pos.x = game.player.pos.x 
	 		+ cos(self.angle) * self.radius
	 	self.pos.y = game.player.pos.y 
	 	 + sin(self.angle) * self.radius
	 	 
			if(self.invincible<1) then
				damageenemies(self)
			end
		
		end,
		draw=function(self)
			if(self.invincible<1) then
				spr(pickframe
					(self.sprites,self.cps)
						,self.pos.x,self.pos.y)
			end
		end,
		damage=function(self)
			self.invincible=self.iframes
		end
	}
end

function damageenemies(self)
	for enemy in all(game.enemies) do
				if(collide(enemy,self)) then
					self:damage()
					enemy:damage()				
				end
			end
end
