//player
function newplayer()
	return {
		pos={x=63,y=63},
		hitbox={x=0,y=0,h=8,w=8},
		speed=2,
		sprites={0,1,2},
		cps=3,
		life=10,
		iframes=30,
		invincible=0,
		firerate=30,
		firecooldown=30,	
		update=function(self)
			if(self.life<1) return
			//move
			if (btn(➡️)) self.pos.x+=self.speed
			if (btn(⬅️)) self.pos.x-=self.speed
			if (btn(⬆️)) self.pos.y-=self.speed
			if (btn(⬇️)) self.pos.y+=self.speed
			//handle iframes
			self.invincible-=1
			
			//shoot
			if(count(game.enemies)>0)then
				if(self.firecooldown>0)then
					self.firecooldown-=1
			else
			 add(game.bullets,newbullet
			 	({x=self.pos.x,y=self.pos.y}
			 	,getunitvector(self, findclosest(self.pos,game.enemies))))
			 self.firecooldown=self.firerate
		 end
		 
 end
		end,
		draw=function(self)
			if(self.life>0) then
				if(self.invincible<1 or rnd(1)>.5) then
					spr(pickframe
						(self.sprites,self.cps)
						,self.pos.x,self.pos.y)			
				end
		else
			print("game over",45,50,8)
		end
	end,
	damage=function(self)
		if(game.player.invincible<1)then
			self.life-=1
			self.invincible=self.iframes
			sfx(11)
		end
	end
	}
end
